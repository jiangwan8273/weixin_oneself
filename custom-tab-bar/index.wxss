.tabbar_box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  background-color: aqua;
  height: calc(100rpx + env(safe-area-inset-bottom));
}

.tabbar_item {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
}

.boxcs {
  position: fixed;
  bottom: calc(100rpx + env(safe-area-inset-bottom));
  height: 100rpx;
  background-color: rgb(226, 59, 59);
  width: 100%;
}