.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.box_flex {
  display: flex;
  justify-content: center;
  align-items: center;
}

.global__fix_tabbar {
  padding-bottom: calc(100rpx + env(safe-area-inset-bottom));
}
